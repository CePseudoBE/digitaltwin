{
  "name": "digitaltwin-cli",
  "version": "0.3.2",
  "description": "CLI tools for Digital Twin projects - generate components, manage projects",
  "license": "MIT",
  "author": "Hoffmann Axel",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dt": "./bin/dt.js"
  },
  "files": [
    "dist/",
    "bin/",
    "stubs/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/bin.ts",
    "clean": "rimraf dist",
    "test": "node --import ts-node-maintained/register/esm bin/test.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "digital-twin",
    "cli",
    "generator",
    "scaffold",
    "make",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CePseudoBE/digitaltwin.git",
    "directory": "digitaltwin-cli"
  },
  "bugs": {
    "url": "https://github.com/CePseudoBE/digitaltwin/issues"
  },
  "homepage": "https://github.com/CePseudoBE/digitaltwin/tree/main/digitaltwin-cli#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^13.4.0",
    "tempura": "^0.4.1",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "@japa/runner": "^4.3.0",
    "@japa/assert": "^4.1.0",
    "ts-node-maintained": "^10.9.5",
    "@types/fs-extra": "^11.0.4"
  }
}
